<h1>List of users</h1>

<table>
	<tr>
		<th>Name</th>
		<th>Email</th>
		<th>Phone</th>
		<th></th>
		<th></th>
	</tr>
	
	<% @users.each do |user| %>
		<tr>
			<td><%= link_to user.name, user_path(user) %></td>
			<td><%= user.email %></td>
			<td><%= user.phone %></td>
			<td><%= link_to "Edit", edit_user_path(user) %></td>
			<td><%= link_to "Delete", user, method: :delete %></td>
		</tr>
	<% end %>
</table>
<br>
<%= link_to "Add new user", new_user_path %>